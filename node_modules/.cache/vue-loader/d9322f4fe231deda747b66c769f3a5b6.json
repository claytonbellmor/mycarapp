v-img\n                    :src=\"car.garageInfo\"\n                    :alt=\"car.info.model\"\n                    height=\"150\"\n                    contain\n                  ></v-img>\n                </template>\n              </v-col>\n            </v-row>\n          </v-card>\n          </v-hover>\n      </v-col>\n\n      <v-col\n        lg=\"3\"\n        md=\"4\"\n        offset-md=\"0\"\n        sm=\"3\"\n        offset-sm=\"2\"\n      >\n        <v-card\n          outlined\n          height=\"225\"\n          class=\"d-flex align-center justify-center blue lighten-3\"\n        >\n          <v-btn>Add New Vehicle</v-btn>\n        </v-card>\n\n      </v-col>\n\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport garage from \"../data/garage.json\"\n\nexport default {\n  name: 'garage',\n  data() {\n    return {\n      garage: garage\n    }\n  },\n  methods: {\n    changeProp(car, cars) {\n      cars.map((e) => {\n        e.selectedCar=null\n      })\n      car.selectedCar=true\n      this.$router.push('/mycar')\n    }\n  }\n}\n</script>"]}]}